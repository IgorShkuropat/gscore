{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "incremental": true,
    "noImplicitAny": false,
  },
  "include": ["next-env.d.ts","fonts.d.ts", "**/*.ts", "pages/_app.jsx", "pages/_app.jsx", "**/*.tsx"],
  "exclude": ["node_modules"]
}
